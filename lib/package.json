{
    "name": "@workleap/netlify-skew-protection",
    "author": "Workleap",
    "version": "1.0.4",
    "description": "Utilities to enable Netlify Skew Protection.",
    "license": "Apache-2.0",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-netlify-skew-protection.git",
        "directory": "lib"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": {
        ".": "./src/index.ts"
    },
    "files": [
        "src",
        "dist",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "dependencies": {
        "@netlify/edge-functions": "^3.0.1"
    },
    "devDependencies": {
        "@eslint/js": "9.38.0",
        "@rslib/core": "0.16.1",
        "@typescript-eslint/parser": "8.46.2",
        "@workleap/eslint-configs": "1.1.2",
        "@workleap/rslib-configs": "1.1.3",
        "@workleap/typescript-configs": "3.0.7",
        "eslint": "9.38.0",
        "msw": "2.11.6",
        "typescript": "5.9.3",
        "typescript-eslint": "8.46.2",
        "vitest": "4.0.5"
    },
    "sideEffects": false
}
