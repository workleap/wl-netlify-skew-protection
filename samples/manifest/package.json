{
    "name": "@samples/manifest",
    "description": "Samples to test a manifest file setup.",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "rsbuild dev --config ./rsbuild.dev.ts",
