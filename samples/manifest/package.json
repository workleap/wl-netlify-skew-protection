        "typecheck": "tsc"
    },
    "devDependencies": {
        "@eslint/js": "9.38.0",
        "@rsbuild/core": "1.5.17",
        "@rspack/core": "1.5.8",
        "@typescript-eslint/parser": "8.46.2",
        "@workleap/browserslist-config": "2.1.3",
        "@workleap/eslint-configs": "1.1.2",
        "@workleap/netlify-skew-protection": "workspace:*",
        "@workleap/rsbuild-configs": "3.1.4",
        "@workleap/typescript-configs": "3.0.7",
        "eslint": "9.38.0",
        "http-server": "14.1.1",
        "typescript": "5.9.3",
        "typescript-eslint": "8.46.2"
    }
