{
    "name": "@samples/manifest",
    "description": "Samples to test a manifest file setup.",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "rsbuild dev --config ./rsbuild.dev.ts",
        "build": "rsbuild build --config ./rsbuild.build.ts",
        "serve-build": "pnpm http-server dist -p 8080 -P http://localhost:8080? -c-1",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc"
    },
    "devDependencies": {
        "@rsbuild/core": "1.3.21",
        "@rspack/core": "1.3.11",
        "@typescript-eslint/parser": "8.32.1",
        "@workleap/browserslist-config": "2.0.4",
        "@workleap/eslint-plugin": "3.4.2",
        "@workleap/netlify-skew-protection": "workspace:*",
        "@workleap/rsbuild-configs": "3.0.2",
        "@workleap/typescript-configs": "3.0.4",
        "eslint": "8.57.1",
        "http-server": "14.1.1",
        "typescript": "5.8.3"
    }
}
